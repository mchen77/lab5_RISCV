

module alu_control (
	input 		[3:0] instr_split,
	input 		[1:0] aluop,
	output reg 	[3:0] aluopcode
)
	
	always@(*) begin
		case(aluop) begin
			2'b00: begin		// S-type (add)
				aluopcode = 4'b0000;
			end
			2'b01: begin		// B-type (sub)
			
			end
			2'b10: begin		// R-type (funct3 business)
			
			end
		
		endcase
	
	end
	
	




endmodule